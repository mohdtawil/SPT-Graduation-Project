<%- include("../../patials/header/firstHeader.ejs") %>
<title>SPT | Insert Unit</title>
<%- include("../../patials/header/styleHeader.ejs") %>
<%- include("../../patials/header/endHeader.ejs") %>
<%- include("../../patials/header/navHeader.ejs") %>
<section id="User">
    <div class="conatiner">
        <div class="row m-0 justify-content-end">
            <div class="col-xl-3 col-lg-4 col-md-4 col-sm-5 col-7 px-xl-4 px-lg-3 px-md-2 mt-md-1 mt-2">
                <a href="/view/units" >
                    <div class="user p-1">
                        <div class="userContainer">
                            <div class="img">
                                <img src="data:image/jpeg;base64," class="userImage" alt="">
                            </div>
                            <div class="text">
                                <p class="userName d-inline"> View All Units</p>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<section id="Title">
    <div class="container">
        <div class="row">
            <div class="col-12 mt-3">
                <div class="row justify-content-center">
                    <div class="col-lg-6 col-md-7 col-sm-8 col-9 text-center">
                        <p class="title">Insert New Unit</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section id="Form">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-7 col-11 mt-5">
                <div class="formContainer p-4 pb-2">
                    <form action="/insert/unit" method="POST">
                        <label for="" class="formLabel">Unit Name</label>
                        <input type="text" name="form_name" placeholder="" class="formInput">
                        
                        <div class="d-flex  mt-3 justify-content-end">
                            <button class="formButton">insert</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>