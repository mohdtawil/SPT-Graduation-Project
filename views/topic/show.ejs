<section id="User">
    <div class="conatiner">
        <div class="row m-0 justify-content-end">
            <div class="col-xl-3 col-lg-4 col-md-4 col-sm-5 col-7 px-xl-4 px-lg-3 px-md-2 mt-md-1 mt-2">
                <a href="/show/topic/<%= singleTopic._id %>/remedialplan" >
                    <div class="user p-1">
                        <div class="userContainer">
                            <div class="img">
                                <img src="data:image/jpeg;base64," class="userImage" alt="">
                            </div>
                            <div class="text">
                                <p class="userName d-inline"> Show Remedials Plan</p>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>

<section id="User">
    <div class="conatiner">
        <div class="row m-0 justify-content-end">
            <div class="col-xl-3 col-lg-4 col-md-4 col-sm-5 col-7 px-xl-4 px-lg-3 px-md-2 mt-md-1 mt-2">
                <a href="/calculate/topic/<%= singleTopic._id %>" >
                    <div class="user p-1">
                        <div class="userContainer">
                            <div class="img">
                                <img src="data:image/jpeg;base64," class="userImage" alt="">
                            </div>
                            <div class="text">
                                <p class="userName d-inline">Claculate AVG</p>
                            </div>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<style>
    .link {
        font-weight: 500;
        color: #fff;
        cursor: pointer;
        border-radius: 20px;
        padding: 5px;
        width: 8em;
        background-color: #3b617c;
    }
    .sl {
        display: none;
    }
    #AVG {
        visibility: hidden;
    }
</style>


<div class="row m-0">
    <div class="col-11 px-5 py-3  d-flex justify-content-end" style="margin-left: 2.5em;">
        <p class="link button1"style="margin-right: -2.3em" ><i class="material-icons" style="font-size:20px">get_app</i> Show AVG </p>
        <p id="AVG"><%= singleTopic.AVG %></p>
    </div>
</div>
<div class="row m-0">
    <div class="sl">
        <div class="col-12  px-5 py-2 d-flex justify-content-end">
            <div id="piechart"></div>
        </div>
    </div>
    
</div>

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<script type="text/javascript">
// Load google charts
google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(drawChart);

// Draw the chart and set the chart values
function drawChart() {
    var avg = Number(document.getElementById("AVG").innerHTML);
    var max = 100 - avg;
  var data = google.visualization.arrayToDataTable([
  ['Task', 'Hours per Day'],
  ['success', avg],
  ['faluire', max],
]);

  // Optional; add a title and set the width and height of the chart
  var options = {'title':'Average For All Students', 'width':230, 'height':180, 'background-color': '#FF0'};

  // Display the chart inside the <div> element with id="piechart"
  var chart = new google.visualization.PieChart(document.getElementById('piechart'));
  chart.draw(data, options);
}
</script>
<script>
     $(".button1").click(() => {
         $(".sl").slideToggle("slow");
    })
</script>
<section id="Title">
    <div class="container">
        <div class="row">
            <div class="col-12 mt-3">
                <div class="row justify-content-center">
                    <div class="col-md-9  text-center">
                        <p class="title"><%= singleTopic.name %></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section id="Refresh">
    <div class="container">
        <div class="row">
            <div class="col-10 p-0 d-flex justify-content-end">
                <a class="reff" href="/refresh/topic/<%= singleTopic._id %>" title="Refresh for update student list for between classroom and this topic">Refresh <i class="fa fa-refresh"></i></a>
            </div>
        </div>
    </div>
</section>
<style>
    .studentList {
        display: none;
       
    }
    .studentList1 {
        display: none;
    }
    .add:hover {
        cursor: pointer;
    }
    .view:hover {
        cursor: pointer;
    }
    .material-icons {
        font-size: 1.5em;
    }
    .stdListItem {
        border-radius: 15px;
        background-color: #327FAD;
        
    }
    .name {
        font-weight: bold;
        color: #FFF;
        font-size: 1.1em;
    }
    .input {
        background-color: rgba(255, 255, 255, 0.39);
           
            border-radius: 5px;
            border: none;
            padding: 4px;
            color: #FFF;
            margin-right: 0.5em;
    }
    .button {
        color: #FFF;
            font-weight: 500;
            background-color: #3958be;
            border: none;
            border-radius: 4px;
            padding: 5px 15px;
            text-transform: capitalize;
    }
</style>
<div class="row m-0">
    <div class="col-12 text-center mt-3">
        <p class="h4">Grade out of <%= singleTopic.maxGrade %></p>
    </div>
</div>
<div class="row m-0">
    <div class="col-6 mt-3  px-5">
        <p class="h3 d-flex justify-content-start align-items-center add"> <i class="material-icons">add</i> Add new grade</p>
        <div class="studentList p-3"> 
            
            <% singleTopic.studentListID.forEach((student) => { %>
                <div class="row">
                    <div class="col-12 p-2 stdListItem my-1">
                        <div class="row">
                            <div class="col-7 ">
                                <span class="name"><%= student.name %> - <%= student.ssn %></span>
                            </div>
                            <div class="col-4 ">
                                <form class="m-0" action="/add/topic/<%= singleTopic._id %>/student/<%= student._id %>/mark" method="POST">
                                    <input type="number" class="input" min="0" max="100" placeholder="Grade" name="form_mark" value="">
                                    <button class="button">save</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
    <div class="col-6  mt-3 px-5 ">
        <p class="h3 d-flex justify-content-end align-items-center view"> <i class="material-icons" style="font-size: 1.1em; padding: 0.3em;">remove_red_eye</i> Show grades</p>
        <div class="studentList1 gradeList p-3 "> 
            <% singleTopic.ListOfMarks.forEach((mark) => { %>
                <div class="row justify-content-end">
                    <div class="col-9 p-2 stdListItem my-1">
                        <div class="row">
                            <div class="col-8">
                                <p class="mb-0"><span class="name"><%= mark.id.name %> -  <%= mark.id.ssn %></span></p>
                                
                                <span class="name text-dark">Mark: <%= mark.mark %></span>
                            </div>
                            <div class="col-4 ">
                                <form class="m-0" action="/delete/topic/<%= singleTopic._id %>/student/<%= mark.id._id %>/mark?_method=DELETE" method="POST">
                                    <button class="button">Delete</button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</div>
<script>
    $(".add").click(() => {
        $(".studentList").slideToggle("slow");
    })
    $(".view").click(() => {
        $(".gradeList").slideToggle("slow");
    })
</script>
<br><br>


